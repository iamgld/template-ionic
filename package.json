{
	"name": "ionic-template",
	"version": "1.0.0",
	"author": "Gregory J Lopez",
	"homepage": "https://iamgld.com",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:open": "ng serve --open",
		"start:host": "ng serve --host 0.0.0.0",
		"start:prod": "ng serve --configuration production",
		"start:ios": "ionic capacitor run ios --livereload",
		"start:android": "ionic capacitor run android --livereload",
		"build": "yarn build:prod",
		"build:prod": "ng build --optimization --configuration production --progress",
		"build:dev": "ng build --optimization --configuration development --progress",
		"build:stats": "ng build --optimization --configuration development --stats-json",
		"build:ios": "ionic capacitor build ios",
		"build:android": "ionic capacitor build android",
		"test": "ng test",
		"lint": "eslint src/app/",
		"lint:fix": "eslint --fix src/app/",
		"lint:ng": "ng lint",
		"lint:ng:fix": "ng lint --fix",
		"analyze": "webpack-bundle-analyzer dist/stats.json",
		"postinstall": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/common": "14.2.7",
		"@angular/core": "14.2.7",
		"@angular/forms": "14.2.7",
		"@angular/platform-browser": "14.2.7",
		"@angular/platform-browser-dynamic": "14.2.7",
		"@angular/router": "14.2.7",
		"@capacitor/app": "4.1.0",
		"@capacitor/core": "4.4.0",
		"@capacitor/haptics": "4.0.1",
		"@capacitor/keyboard": "4.0.1",
		"@capacitor/status-bar": "4.0.1",
		"@ionic/angular": "6.3.2",
		"@ngrx/effects": "14.3.2",
		"@ngrx/store": "14.3.2",
		"@ngrx/store-devtools": "14.3.2",
		"@ngrx/router-store": "14.3.2",
		"@ngrx/schematics": "14.3.2",
		"ionicons": "6.0.3",
		"rxjs": "7.5.7",
		"tslib": "2.4.0",
		"zone.js": "0.11.8"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "14.2.6",
		"@angular-eslint/builder": "14.1.2",
		"@angular-eslint/eslint-plugin": "14.1.2",
